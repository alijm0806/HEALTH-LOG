@vitamins.map do |vitamin|
  {
    id: vitamin.id,
    name: vitamin.name,
    image_url: vitamin.image_url,
    deficiency: vitamin.deficiency,
    created_at: vitamin.created_at,
    updated_at: vitamin.updated_at,
    images: vitamin.images,
    used_for: vitamin.used_for,
    users: vitamin.users,
    stats: User.all.count,
    list_of_vitamins: vitamin.list_of_vitamins,

  }
end
